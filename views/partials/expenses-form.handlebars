<div class="column">
        <ul id="expenses-list">
            {{#each this}}
                <li>{{title}} {{cost}}</li>
            {{/each}}
        </ul>
    </div>
    <div class="column">
        <div id="form-display">
            <form class="addExpenseForm">
                <div>
                    <label for="expense-name">Name of Expense:</label><br>
                    <input type="text" name="expense-name" id="expense-name">
                </div>
                <div>
                    <label for="expense-frequency">Frequency:</label><br>
                    <input type="text" name="expense-frequency" id="expense-frequency">
                </div>
                <div>
                    <label for="expense-cost">Cost:</label><br>
                    <input type="text" name="cost" id="expense-cost">
                </div>
                <div>
                <div id="expense-priority" class="radio-display">
                    <input type="radio" class="radio" name="priority" value="Primary" id="primary" />
                    <label for="primary">Primary</label>
                    <input type="radio" class="radio" name="priority" value="Secondary" id="secondary" />
                    <label for="secondary">Secondary</label>
                </div>
                </div>
                <div>
                    <button type="submit">Add Expense</button>
                </div>
            </form>
        </div>
    </div>